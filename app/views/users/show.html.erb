<div class="booyah-box col-xs-10 col-xs-offset-1">
  <div class="col-xs-6">
    <img src="#" alt="image">
      <h3><%= @user.email %></h3>
  </div>
  <div class="col-xs-6">
    <h4>
      Member since: <%= @user.created_at.strftime("%B %d, %Y") %>
    </h4>
    <hr />
    <p><%= @user.comments.count %> Total Comments</p>
    <p><%= @user.comments.where(rating: '5_stars').count %> Total 5 Star Reviews</p>
  </div>

</div>